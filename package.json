{
  "name": "testsolo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "npm run build:backend && nodemon --watch api --ext ts --exec \"npm run build:backend && node api/dist/server.js\"",
    "build": "npm run build:backend && vite build",
    "build:backend": "tsc --project api/tsconfig.json",
    "start": "npm run build:backend && node api/dist/server.js",
    "start:prod": "node api/dist/server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc --noEmit"
  },
